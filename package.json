    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.11",
    "db-migrate": "^0.11.1",
    "db-migrate-mysql": "^1.1.10",
    "directory-named-webpack-plugin": "^2.2.3",
    "easywebpack-cli": "^3.5.2",
    "easywebpack-vue": "^3.6.0",
