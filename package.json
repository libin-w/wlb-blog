{
  "name": "blog-system",
  "version": "3.0.0",
  "description": "基于 easywebpack-vue 和 egg-view-vue-ssr插件的 egg + vue 服务端渲染同构工程骨架项目",
  "scripts": {
    "react:dev": "cross-env NODE_ENV=development webpack --config ./webpack.config.react.js",
    "react:watch": "cross-env NODE_ENV=development webpack --watch --config ./webpack.config.react.js",
    "react:pro": "rm -rf public/chat_app/ && cross-env NODE_ENV=production webpack -p --config ./webpack.config.react.js",
    "debug": "egg-bin debug",
    "build": "cross-env easy clean && easy build prod",
    "build:dev": "cross-env easy clean && easy build dev",
    "build:test": "cross-env easy clean && easy build test",
    "build:prod": "cross-env easy clean && easy build prod",
    "start": "node index.js",
    "start:test": "cross-env EGG_SERVER_ENV=test node index.js",
    "start:prod": "cross-env EGG_SERVER_ENV=prod NODE_ENV=production npm start",
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "migrate:create": "db-migrate create --config config/database.json",
    "migrate": "db-migrate up --config config/database.json -e dev",
    "migrate:prod": "db-migrate up --config config/database.json -e prod",
    "migrate:rollback": "db-migrate down --config config/database.json"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "egg": "^2.1.0",
    "egg-bin": "^4.3.7",
    "egg-cors": "^2.0.0",
    "egg-logger": "^1.5.0",
    "egg-mysql": "^3.0.0",
    "egg-validate": "^1.0.0",
    "egg-view-nunjucks": "^2.2.0",
    "egg-view-vue-ssr": "^3.0.2",
    "element-ui": "^2.3.2",
    "extend": "~3.0.0",
    "iview": "^2.10.1",
    "lodash": "^4.17.4",
    "mint-ui": "^2.2.9",
    "mockjs": "^1.0.1-beta3",
    "moment": "^2.17.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "vue": "^2.5.0",
    "vue-hot-reload-api": "^2.1.0",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.3",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autod-egg": "^1.0.0",
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
